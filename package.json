{
  "name": "angular-pipes",
  "version": "12.0.0",
  "description": "Angular pipes library",
  "scripts": {
    "ng": "ng",
    "cover": "cat ./coverage/*/lcovonly | ./node_modules/.bin/coveralls",
    "karma": "node_modules/.bin/karma start karma.conf.js",
    "build": "ng-packagr -p package.json",
    "publish-dist": "npm publish dist",
    "test": "npm run karma",
    "lint": "run-p -c lint:*",
    "lint:pretty": "prettier '**/*.{ts,scss,js,json,yml,md,html}' --list-different",
    "lint:src": "eslint 'src/**/**.ts' --ignore-pattern 'src/**/*.spec.ts'",
    "lint:spec": "eslint 'test/**/**.spec.ts' -c '.eslintrc.spec.js'"
  },
  "author": "Wizbii",
  "repository": {
    "type": "git",
    "url": "https://github.com/wizbii/angular-pipes.git"
  },
  "keywords": [
    "ng2",
    "ng",
    "angular",
    "angular2",
    "angularjs",
    "pipes",
    "filters",
    "library"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cli": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/platform-server": "^11.2.14",
    "@types/jasmine": "^3.3.16",
    "core-js": "^3.1.4",
    "coveralls": "^3.0.5",
    "jasmine-core": "^3.4.0",
    "karma": "^4.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^4.1.1",
    "ng-packagr": "^5.4.3",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "typescript": "4.0.7",
    "zone.js": "~0.10.3"
  },
  "dependencies": {},
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    }
  }
}